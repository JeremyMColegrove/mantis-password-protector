{
  "name": "mantis-password-protector",
  "version": "1.0.12",
  "description": "Simple, easy to use and secure password manager",
  "author": "Jeremy Colegrove",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "https://github.com/JeremyMColegrove/mantis-password-protector.git"
  },
  "main": "public/electron.js",
  "dependencies": {
    "@electron/remote": "^2.0.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "electron-context-menu": "^3.1.2",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^4.6.5",
    "json-2-csv": "^3.17.0",
    "owasp-password-strength-test": "^1.3.0",
    "react": "^17.0.2",
    "react-autocomplete-hint": "^1.3.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "dist": "react-scripts build && electron-builder ",
    "publish": "react-scripts build && electron-builder --win -p always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "electron": "^17.0.1",
    "electron-builder": "^22.14.13",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "cross-env": "^7.0.3",
    "concurrently": "^7.0.0",
    "wait-on": "^6.0.1"
  }
}
